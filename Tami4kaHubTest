TargetSection:NewButton("ğŸ“’ Copy DisplayName ğŸ“’","ĞšĞ¾Ğ¿Ğ¸Ñ€ÑƒĞµÑ‚ Ğ´Ğ¸ÑĞ»ĞµĞ¹ Ğ¸Ğ¼Ñ Ğ¸Ğ³Ñ€Ğ¾ĞºĞ°", function()
