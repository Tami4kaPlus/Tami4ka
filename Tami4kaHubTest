OthersSection:NewTextBox("⬛ Teleport Place ⬛", "Enter place id", id, function()
    local PlaceID = id
    game:GetService("TeleportService"):SetTeleportGui(TeleportingUI)
    TeleportService = game:GetService("TeleportService")
    TeleportService:Teleport(PlaceID, game.Players.LocalPlayer, game.PlaceId)
end)

