local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Tami4kaPlus/Tami4ka/main/TestLib"))()

local Window = Library.CreateLib("üçá Tami4ka Hub ü•ù", "GrapeTheme", "rbxassetid://17601879017")

local coordinates
_G.HeadSize = 15

local PlrTab = Window:NewTab("Main")
local PlrSection = PlrTab:NewSection("Main")

PlrSection:NewSlider("‚ö° Speed ‚ö°", "Set you speed", 150, 16, function(s)
  game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

PlrSection:NewSlider("üîù Jump Power üîù", "Set you JumpPower", 250, 50, function(s)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)

PlrSection:NewSlider("‚öô Hip Height ‚öô", "Set you HipHeight", 75, 0, function(s)
game.Players.LocalPlayer.Character.Humanoid.HipHeight = s
end)

PlrSection:NewSlider("üëÄ Field Of View üëÄ", "Set you FOV", 120, 70, function(s)
game.Workspace.CurrentCamera.FieldOfView = s
end)

PlrSection:NewButton("üë§ Save Coordinates üë§", "Save you Coordinates", function()
coordinates = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
end)

PlrSection:NewButton("üëÅÔ∏è Teleport Coordinates üëÅÔ∏è", "Tp saved Coordinates", function ()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = coordinates
end)

PlrSection:NewButton("‚úî Teleport to click ‚úî", "Teleport", function()
  mouse = game.Players.LocalPlayer:GetMouse()
  tool = Instance.new("Tool")
  tool.RequiresHandle = false
  tool.Name = "Click Teleport"
  tool.Activated:connect(function()
  local pos = mouse.Hit+Vector3.new(0,2.5,0)
  pos = CFrame.new(pos.X,pos.Y,pos.Z)
  game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
  end)
  tool.Parent = game.Players.LocalPlayer.Backpack
end)

PlrSection:NewButton("üåå FullBrightness üåå", "–î–µ–ª–∞–µ—Ç –æ—Å–≤–∏—â–µ–Ω–∏–µ —è—Ä–∫–∏–º", function()
  local Light = game:GetService("Lighting")
  function dofullbright()
    Light.Ambient = Color3.new(1, 1, 1)
    Light.ColorShift_Bottom = Color3.new(1, 1, 1)
    Light.ColorShift_Top = Color3.new(1, 1, 1)
  end
  dofullbright()
  Light.LightingChanged:Connect(dofullbright)
end)

PlrSection:NewSlider("‚èπÔ∏è Hitbox Scale ‚èπÔ∏è", "Set you speed", 50, 0, function(s)
  _G.HeadSize = s
end)

PlrSection:NewToggle("üéØ Expander [ HumanoidRootPart ] üéØ", "Hitbox Expander", function(toggle)
 if toggle then
    _G.Hitbox = true
     dayn = game:GetService("RunService").RenderStepped:Connect(function()
      for i,v in next, game:GetService('Players'):GetPlayers() do
        if v.Name ~= game:GetService('Players').LocalPlayer.Name then
        pcall(function()
          v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize)
          v.Character.HumanoidRootPart.Transparency = 0.7
          v.Character.HumanoidRootPart.BrickColor = BrickColor.new("Really black")
          v.Character.HumanoidRootPart.Material = "Neon"
          v.Character.HumanoidRootPart.CanCollide = false
        end)
        end
      end
    end)
    repeat task.wait() until _G.Hitbox == false
    dayn:Disconnect()
 else
    _G.Hitbox = false
 end
end)

PlrSection:NewToggle("üéØ Expander [ Torso R6 ] üéØ", "Hitbox Expander", function(toggle)
 if toggle then
    _G.HitboxT = true
     dayn = game:GetService("RunService").RenderStepped:Connect(function()
      for i,v in next, game:GetService('Players'):GetPlayers() do
        if v.Name ~= game:GetService('Players').LocalPlayer.Name then
        pcall(function()
          v.Character.Torso.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize)
          v.Character.Torso.Transparency = 0.7
          v.Character.Torso.BrickColor = BrickColor.new("Really black")
          v.Character.Torso.Material = "Neon"
          v.Character.Torso.CanCollide = false
        end)
        end
      end
    end)
    repeat task.wait() until _G.Hitbox == false
    dayn:Disconnect()
 else
    _G.HitboxT = false
 end
end)

PlrSection:NewToggle("üéØ Expander [ UpperTorso R15 ] üéØ", "Hitbox Expander", function(toggle)
 if toggle then
    _G.HitboxT = true
     dayn = game:GetService("RunService").RenderStepped:Connect(function()
      for i,v in next, game:GetService('Players'):GetPlayers() do
