local lp = game:GetService("Players").LocalPlayer

local camera = workspace.CurrentCamera
local RunService = game:GetService("RunService")

function animate(idle1, idle2, walk, run, jump, climb, fall)
    game.Players.LocalPlayer.Character:WaitForChild("Animate", math.huge).idle.Animation1.AnimationId = idle1
    game.Players.LocalPlayer.Character:WaitForChild("Animate", math.huge).idle.Animation2.AnimationId = idle2
    game.Players.LocalPlayer.Character:WaitForChild("Animate", math.huge).walk.WalkAnim.AnimationId = walk
    game.Players.LocalPlayer.Character:WaitForChild("Animate", math.huge).run.RunAnim.AnimationId = run
    game.Players.LocalPlayer.Character:WaitForChild("Animate", math.huge).jump.JumpAnim.AnimationId = jump
    game.Players.LocalPlayer.Character:WaitForChild("Animate", math.huge).climb.ClimbAnim.AnimationId = climb
    game.Players.LocalPlayer.Character:WaitForChild("Animate", math.huge).fall.FallAnim.AnimationId = fall
end

animate("http://www.roblox.com/asset/?id=1083445855","http://www.roblox.com/asset/?id=1083450166","http://www.roblox.com/asset/?id=742640026","http://www.roblox.com/asset/?id=742638842","http://www.roblox.com/asset/?id=742637942","http://www.roblox.com/asset/?id=742636889","http://www.roblox.com/asset/?id=742637151")

-- Auto-Sell requires
local getPlayerData = game:GetService("ReplicatedStorage"):FindFirstChild("GameNetwork"):FindFirstChild("ServerNetwork"):FindFirstChild("GetPlayerData");
local sellClothing = game:GetService("ReplicatedStorage"):FindFirstChild("GameNetwork"):FindFirstChild("ServerNetwork"):FindFirstChild("SellClothing");

local itemsSold = 0;
local sellDelay = 5;

---
local ClothTable = {}
--- TOGGLES
_G.Auto_Sell = false
--Esp Stores
_G.Esp = false

_G.Sport_Master = false
_G.Adidas = false
_G.Bape = false
_G.Chrome_Hearts = false
_G.Vetements = false
_G.Zara = false
_G.StoneIsland =false
_G.Balenciaga = false
_G.LouisV = false
_G.Gucci = false
_G.TNF = false

function getchar()
	local character = lp.Character
	if character then
		return character
	end
end

function DrawEsp(cloth)
    local Draw = Instance.new("BoxHandleAdornment")
	Draw.Parent = cloth
	Draw.Adornee = cloth
    Draw.Color3 = Color3.fromRGB(255, 0, 255)
    Draw.AlwaysOnTop = true
    Draw.ZIndex = 0
    Draw.Transparency = 0.3
    Draw.Size = Vector3.new(1,2,0.2)
end


function Cloth(PriceTable,NameTable)

    for i, v in pairs(game:GetService("Workspace").World.Debris.Stores:GetDescendants()) do
        if v:FindFirstChildOfClass("ProximityPrompt") and v:FindFirstChild("Clothing") and v:FindFirstChildOfClass("ProximityPrompt").HoldDuration == 1 then
            local ClothPrice = string.gsub(v.ProximityPrompt.ActionText,"%D+","")
            local ClothName = v.ProximityPrompt.ObjectText
            local ClothEsp = v.Clothing

            if v.ProximityPrompt.ActionText == "Повесить" and ClothEsp:FindFirstChild("BoxHandleAdornment") then
                ClothEsp.BoxHandleAdornment:Remove()
            end
         
            if ClothPrice ~= nil and ClothName ~= nil and ClothEsp ~= nil then
                if v.ProximityPrompt.ActionText ~= "Повесить" and ClothEsp:FindFirstChild("BoxHandleAdornment") == nil and table.find(NameTable,ClothName) and tonumber(ClothPrice) <= PriceTable[ClothName] then
                    DrawEsp(ClothEsp)
                end
          
            end
    

        end
    end
end

function AutoSell()

    if itemsSold >= 10 then
        task.wait(5)
        itemsSold = 0
    end
    
    local playerData = getPlayerData:InvokeServer()
    
    for name,data in pairs(playerData) do
        if name ~= "Inventory" or type(data) ~= "table" then
            continue
        end
        
        for _,itemData in pairs(data) do
            local itemName = itemData["ItemName"]
            local itemHash = itemData["ItemHash"]
            local itemPrice = itemData["Price"]

            if itemName == nil or itemHash == nil or itemPrice == nil then
                continue
            end
            sellClothing:InvokeServer(itemHash);
            itemsSold += 1
            
        end
    end

    

end

function AutoJob()
    local player = game.Players.LocalPlayer
    local teleportPosition = Vector3.new(100, 180, 308)
    local tweenInfo = TweenInfo.new(10, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
    local tween = game:GetService("TweenService"):Create(player.Character:WaitForChild("HumanoidRootPart"), tweenInfo, {CFrame = CFrame.new(teleportPosition)})

    tween:Play()

    tween.Completed:Connect(function()
        player.Character:WaitForChild("Humanoid"):MoveTo(workspace.World.Debris.PointJob.Position)
        task.wait(2)
        for i = 1, 5 do
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.World.Debris.FinishJob.CFrame
            wait(0.1)
        end
    end)
end


