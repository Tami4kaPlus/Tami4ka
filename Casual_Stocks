Esp:AddToggle({
	Name = "Dropped Clothes",
	Default = false,
	Callback = function(value)
        if value then 
			_G.DroppedClothes = true
			while _G.DroppedClothes do
                wait(1)
                for i, temp1 in workspace.World.Debris:GetChildren() do
                    if temp1:IsA("Model") then
                        for i, temp2 in temp1:GetChildren() do
                            if temp2:FindFirstChild("ProximityPrompt") then
                                if temp1:FindFirstChild("BoxHandleAdornment") then
                                else
                                    local Draw = Instance.new("BoxHandleAdornment")
                                    Draw.Parent = temp1
                                    Draw.Adornee = temp1
                                    Draw.Color3 = Color3.fromRGB(155, 0, 155)
                                    Draw.AlwaysOnTop = true
                                    Draw.ZIndex = 0
                                    Draw.Transparency = 0.3
                                    Draw.Size = Vector3.new(3.7, 0.2, 2.5)
                                end
                            end
                        end
                    end
                end
            end         
		else
			_G.DroppedClothes = false
		end 
	end    
})

Esp:AddToggle({
	Name = "Esp",
	Default = false,
	Callback = function(value)
        if value then 
			_G.Esp = true
			while _G.Esp do
                if _G.Sport_Master then
                    Cloth(Sport_cloth_PriceTable,Sport_cloth_NameTable)
                end
                if _G.Adidas then
                    Cloth(Adidas_cloth_PriceTable,Adidas_cloth_NameTable)
                end
                if _G.Bape then
                    Cloth(Bape_cloth_PriceTable,Bape_cloth_NameTable)
                end
                if _G.Chrome_Hearts then
                    Cloth(ChromeHearts_cloth_PriceTable,ChromeHearts_cloth_NameTable)
                end
                if _G.Vetements then
                    Cloth(Vetements_cloth_PriceTable,Vetements_cloth_NameTable)
                end
                if _G.Zara then
                    Cloth(Zara_cloth_PriceTable,Zara_cloth_NameTable)
                end

                if _G.StoneIsland then
                    Cloth(StoneIsland_cloth_PriceTable,StoneIsland_cloth_NameTable)
                end

                if _G.LouisV then
                    Cloth(LouisVuitton_cloth_PriceTable,LouisVuitton_cloth_NameTable)
                end

                if _G.Gucci then
                    Cloth(Gucci_cloth_PriceTable,Gucci_cloth_NameTable)
                end
                if _G.Balenciaga then
                    Cloth(Balenciaga_cloth_PriceTable,Balenciaga_cloth_NameTable)
                end
                if _G.TNF then
                    Cloth(TNFPriceTable, TNFNameTable)
                end


                task.wait(3)
            end         
		else
			_G.Esp = false
		end 
	end    
})

